fn main() {
    let k: i64 = 2;
    let n: i64 = 50;
    let nums: Vec<i64> = vec![15, 8];
    let mut s: Vec<i64> = vec![0; k as usize + 1];
    s[0] = n;
    
    for j in 0..k as usize {
        let temp: Vec<i64> = s.clone();
        for i in 1..=j + 1 {
            s[i] += temp[i - 1]/nums[j];
        }
    }
    
    let mut res: i64 = 0;
    let mut idx: i64 = 1;
    for i in 0..=k as usize {
        res += s[i]*idx;
        idx *= -1
    }
    
    println!("{:?}", res)
}
